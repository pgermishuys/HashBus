<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<!--startcode subscriptionConfig-->
<configuration>
  <configSections>
    <section name="UnicastBusConfig" type="NServiceBus.Config.UnicastBusConfig, NServiceBus.Core"/>
  </configSections>
  <UnicastBusConfig>
    <MessageEndpointMappings>
      <add Assembly="HashBus.Application.Events" Type="HashBus.Application.Events.UserMentionedWithHashtag" Endpoint="HashBus.Application"/>
    </MessageEndpointMappings>
  </UnicastBusConfig>
  <appSettings>
    <add key="RavenDBUrl" value="http://localhost:8080/" />
    <add key="RavenDBDatabase" value="Hashbus" />
    <add key="hashTag" value="Simulated" />
    <add key="refreshInterval" value="10000" />
  </appSettings>
</configuration>
<!--endcode-->
